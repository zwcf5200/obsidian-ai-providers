{
	"name": "obsidian-ai-providers",
	"version": "1.3.0",
	"description": "A hub for setting AI providers (OpenAI-like, Ollama and more) in one place.",
	"main": "dist/main.js",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"dev-test": "concurrently \"node esbuild.config.mjs\" \"npm run test:watch\"",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"example:build": "cd packages/example-plugin && npm run build",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"sdk:build": "cd packages/sdk && npm run build",
		"sdk:version": "cd packages/sdk && npm version",
		"sdk:publish": "cd packages/sdk && npm run build && npm publish"
	},
	"keywords": [
		"obsidian",
		"ollama",
		"ai",
		"plugin",
		"llm",
		"gpt",
		"openai",
		"openrouter",
		"gemini",
		"lmstudio",
		"providers",
		"sdk"
	],
	"author": "Pavel Frankov",
	"license": "MIT",
	"dependencies": {
		"ollama": "0.5.10",
		"openai": "4.73.1"
	},
	"devDependencies": {
		"@obsidian-ai-providers/sdk": "^1.1.1",
		"@testing-library/jest-dom": "6.4.2",
		"@types/jest": "29.5.12",
		"@types/node": "16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"concurrently": "8.2.2",
		"electron": "33.2.1",
		"esbuild": "0.17.3",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"obsidian": "latest",
		"ts-jest": "29.1.2",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
